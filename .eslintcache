eturn response\r\n}, error => {\r\n    const isAccessToNetwork = window.navigator.onLine;\r\n    const isServerInaccessible = isAccessToNetwork && error.message === 'Network Error';\r\n    if(isServerInaccessible) {\r\n        store.dispatch(serverInaccessible(true))\r\n    }\r\n    return Promise.reject(error)\r\n})\r\n\r\n\r\ninstance.defaults.headers.common['Authorization'] = AUTH_TOKEN;\r\ninstance.defaults.headers.common['refresh_token'] = REFRESH_TOKEN;\r\ninstance.defaults.headers.common['Access-Control-Allow-Origin'] = '*';\r\n\r\nexport const updateDefaultRequestHeaders = (token, refreshToken) => {\r\n    instance.defaults.headers.common['Authorization'] = token;\r\n    instance.defaults.headers.common['refresh_token'] = refreshToken;\r\n}\r\n\r\n\r\n","C:\\Users\\vital\\Desktop\\todo_list_react\\src\\assets\\svg\\IconPin.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\API\\tasks\\subtasks.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\API\\tasks\\comments.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\API\\settings\\settings.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\settings\\ProfileSettings.jsx",["687"],"import React from 'react';\r\nimport { Biography } from './biography/Biography';\r\nimport { ChangeLanguage } from './change_language/ChangeLanguage';\r\nimport { PersonalData } from './personal_data/PersonalData';\r\nimport { AiOutlineClose } from 'react-icons/ai';\r\nimport { NavLink } from 'react-router-dom';\r\n\r\n\r\n\r\nexport function ProfileSettings() {\r\n    const localState = {\r\n                    state: {\r\n                        biography: React.useState(false),\r\n                        personalData: React.useState(false),\r\n                        changeLanguage: React.useState(false)\r\n                    },\r\n                    openHandler: function(requestPart) {\r\n                        return () => {\r\n                            for(let key in this.state) {\r\n                                const stateValue = this.state[key][0]\r\n                                const setStateValue = this.state[key][1]\r\n                                if(key === requestPart) {\r\n                                    setStateValue(!stateValue)\r\n                                    continue\r\n                                }\r\n                                setStateValue(false)\r\n                            }\r\n                        }\r\n                       \r\n                    },\r\n                    getStateValue: function(requestPart) {\r\n                        return this.state[requestPart][0]\r\n                    }\r\n                };\r\n\r\n    return (\r\n\r\n        <div class=\"settings settings_theme_dark\">\r\n            <NavLink to = '/app' class=\"settings__close-btn\">\r\n                <AiOutlineClose className = 'settings__icon' />\r\n            </NavLink>\r\n            <h2 class=\"settings__header\">Settings</h2>\r\n            <ul class=\"settings__parts-list\">\r\n                <Biography onOpen = {localState.openHandler('biography')} \r\n                           isOpen = {localState.getStateValue('biography')} />\r\n\r\n                <PersonalData onOpen = {localState.openHandler('personalData')} \r\n                           isOpen = {localState.getStateValue('personalData')} />\r\n\r\n                {/* <ChangeLanguage onOpen = {localState.openHandler('changeLanguage')} \r\n                           isOpen = {localState.getStateValue('changeLanguage')} /> */}\r\n            </ul>\r\n        </div>\r\n    )\r\n}","C:\\Users\\vital\\Desktop\\todo_list_react\\src\\API\\biography\\biography.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\redux\\actions\\personal_data\\personal_data.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\API\\personal_data\\personal_data.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\redux\\reducers\\personal_data\\personal_data.js",[],"