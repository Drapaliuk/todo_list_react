 <SortingOrder sortByHandler = {sortByHandler} sortOrder = {sortOrder} sortBy = {sortBy} />\r\n        </ul>\r\n\r\n    )\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n// const possibleSortedBy = [\r\n//     {\r\n//         sortBy: 'text/asc',\r\n//         title: 'Sort Alphabetically',\r\n//         icon: AiOutlineSortAscending\r\n//     },\r\n//     {\r\n//         sortBy: 'dateCreation/asc',\r\n//         title: 'Sort by Creation Date',\r\n//         icon: BsCalendar\r\n//     },\r\n//     {\r\n//         sortBy: 'isImportant/asc',\r\n//         title: 'Sort by Priority',\r\n//         icon: AiFillStar\r\n//     },\r\n// ]","C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\tasks_list\\todo_list_settings\\Settings.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\tasks_list\\todo_list_settings\\SortingOption.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\tasks_list\\todo_list_settings\\SortingOrder.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\tasks_list\\todo_list_settings\\SettingsOption.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\index.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\index.js",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\comment\\Comment.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\subtask\\SubTask.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\comments\\Comments.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\due_time\\DueTime.jsx",[],"C:\\Users\\vital\\Desktop\\todo_list_react\\src\\components\\organizer\\task_full_info\\full_info_manipulations\\FullInfoManipulations.jsx",["683"],"import React from 'react'\r\nimport { BsTrash } from 'react-icons/bs'\r\nimport